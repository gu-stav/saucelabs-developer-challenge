{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-mixed-operators": "off",
    "prettier/prettier": [
                "error",
                {
                    "singleQuote": true,
                },
    ],
  },
  "env": {
    "browser": true,
  },
}
